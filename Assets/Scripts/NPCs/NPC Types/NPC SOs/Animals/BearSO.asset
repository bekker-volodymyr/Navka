    \u0439\u043E\u0433\u043E \u043A\u0440\u043E\u043A \u2014 \u044F\u043A \u0433\u043B\u0438\u0431\u043E\u043A\u0435
    \u0432\u0456\u0434\u0434\u0437\u0435\u0440\u043A\u0430\u043B\u0435\u043D\u043D\u044F
    \u0432\u0435\u043B\u0438\u0447\u0456 \u0433\u0456\u0440 \u0442\u0430 \u0442\u0430\u0454\u043C\u043D\u0438\u0446\u044C
    \u0434\u0430\u0432\u043D\u0456\u0445 \u0447\u0430\u0441\u0456\u0432. \u0412\u0435\u0434\u043C\u0456\u0434\u044C
    \u043D\u0435 \u0448\u0443\u043A\u0430\u0454 \u0441\u0432\u0430\u0440\u043E\u043A,
    \u0432\u0456\u043D \u043C\u0443\u0434\u0440\u043E \u0432\u0438\u0431\u0438\u0440\u0430\u0454
    \u0442\u0438\u0445\u0438\u0439 \u0448\u043B\u044F\u0445, \u0434\u0435 \u0439\u043E\u0433\u043E
    \u0432\u0435\u043B\u0438\u0447 \u0441\u0442\u0430\u0454 \u0432\u0438\u0440\u0430\u0437\u043D\u0438\u043A\u043E\u043C
    \u043C\u0438\u0440\u043D\u043E\u0433\u043E \u0441\u043F\u0456\u0432\u0456\u0441\u043D\u0443\u0432\u0430\u043D\u043D\u044F
