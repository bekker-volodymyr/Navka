    \u0432\u0435\u043B\u0438\u0447\u043D\u0438\u0439 \u0432\u0435\u0434\u043C\u0456\u0434\u044C.
    \u0412 \u0439\u043E\u0433\u043E \u043A\u043E\u0441\u043C\u0430\u0442\u043E\u043C\u0443
    \u043F\u043E\u043A\u0440\u0438\u0432\u0456 \u0445\u043E\u0432\u0430\u0454\u0442\u044C\u0441\u044F
    \u0442\u0435\u043F\u043B\u043E \u0456 \u0441\u0438\u043B\u0430 \u0441\u0442\u043E\u043B\u0456\u0442\u044C,
    \u0430 \u0432 \u043E\u043A\u0430\u0445 \u0439\u043E\u0433\u043E \u2014 \u043C\u0443\u0434\u0440\u0456\u0441\u0442\u044C,
    \u044F\u043A\u0430 \u0437\u0431\u0435\u0440\u0435\u0433\u043B\u0430 \u0442\u0430\u0454\u043C\u043D\u0438\u0446\u0456
    \u043B\u0456\u0441\u043E\u0432\u043E\u0433\u043E \u0441\u0432\u0456\u0442\u0443.
    \u0412\u0435\u0434\u043C\u0456\u0434\u044C \u043D\u0435 \u043F\u0440\u043E\u0441\u0442\u043E
    \u0442\u0432\u0430\u0440\u0438\u043D\u0430, \u0432\u0456\u043D \u2014 \u0432\u0442\u0456\u043B\u0435\u043D\u043D\u044F
